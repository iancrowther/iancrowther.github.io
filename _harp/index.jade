div.container
	div.row
		div.col-xs-12.col-sm-9.col-md-9.col-lg-9
			h1= title
			h2= 'Availability: ' + contracts.availability.closed
			h3= contracts.agencies

		div.col-xs-12.col-sm-3.col-md-3.col-lg-3
			h3 Contact
			ul.fa-ul
				each item in contact
					li: a(href= item.href, target="_blank")
						i(class="fa-li fa fa-lg " + item.icon)
						= item.text
			
		div.col-xs-12.col-sm-9.col-md-9.col-lg-9
			h1= projects.hero
			ul
				each consultancy in projects.consultancies
					a(href=consultancy.url, title=consultancy.name)
						li= consultancy.name